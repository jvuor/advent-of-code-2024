{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "arrow-parens": false,
      "interface-name": false,
      "no-console": false,
      "no-var-requires": false,
      "prefer-for-of": false,
      "quotemark": [ true, "single" ],
      "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
